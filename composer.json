{
    "name": "zai/laravel-eloquent-multilingualization",
    "description": "Add multilingual support to laravel eloquent models",
    "license": "MIT",
    "authors": [
        {
            "name": "Zai chao Pan",
            "email": "zaichaopan@gmail.com"
        }
    ],
    "require": {
       "php": ">=5.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.0",
        "orchestra/database": "~3.4.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Zai\\Translate\\": "src/"
        },
        "classmap": [
            "tests"
        ]
  },
  "extra": {
    "laravel": {
        "providers": [
            "Zai\\Translate\\TranslationServiceProvider"
        ]
    }
  },
   "minimum-stability": "dev"
}
